    }), $("#hdr_search").focus(function () {
        "?" == $(this).val() && $(this).val("")
    }), $("#hdr_search").blur(function () {
        "" == $(this).val() && $(this).val("?")
    }), $(".filtr .arrow2").live("click", function () {
