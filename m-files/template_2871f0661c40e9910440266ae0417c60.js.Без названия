top.loadPriceAlerts = function(){
	$(".fs-pricealert-product").each(function(){
		var id = $(this).attr("data-id") || "";
		if(id.length > 0)
		{
			$(this).load("/ajax/pricealert.php",{"id":id},function(){});
		}
	});
};

top.fsLoadPrices = function(container){
	container = container || "";
	var ids = [];
	$(container + " .price-load-block").each(function(){
        if ($(this).attr("data-id") != undefined)
            ids.push($(this).attr("data-id"));
	});
	if(ids.length > 0)
	{
		$.get("/ajax.php",{"action":"getPrices","ID":ids},function(data){
			if(typeof data.PRICES === "object") {
				for (key in data.PRICES) {
					if (data.PRICES.hasOwnProperty(key)) {
						var price = data.PRICES[key];
						var priceBlock = $(".price-load-block[data-id="+price.PRODUCT_ID+"]");
						priceBlock.find(".fs-price-value").html(price.PRICE_F);
						priceBlock.find(".button2.ajax_link").attr('data-price',price.PRICE);
						priceBlock.parent().find(".fs-buy-btn").data('price',price.PRICE);
                        if (typeof priceBlock.find(".fs-price-value").data("fbq")!=="undefined") {
                            fbq('track', 'ViewContent',priceBlock.find(".fs-price-value").data("fbq"));
						}
					}
				}
			}
		},"json");
	}
};

top.fsAjaxLoad = function (container, ajax_call, params, callback) {
	$(container).addClass("fs-loading-bg");
	$.post('/bitrix/tools/ajax.php?ajax_call='+ajax_call, params, function (data) {
        if (container) {
            $(container).removeClass("fs-loading-bg");
            $(container).html(data);
            if(typeof callback === "function") callback();
        }
	});
};

top.updateBasketBlock = function updateBasketBlock(){
	$.get("/ajax.php",{"action":"updateBasketBlock"},function(html){
		$("#fs-basket-block").replaceWith(html);
	});
};
top.DelStyleWidth = function(){
    var styleAttr = $(".content_left").attr('style');
    if (typeof styleAttr !== typeof undefined && styleAttr !== false) {
        $(".content_left").removeAttr('style');
    }
};
$(function(){
	
	top.loadPriceAlerts();
	top.fsLoadPrices("body");
    top.DelStyleWidth();

	$(document).on("click",".ajax_link",function(e){ // Обработка ссылок AJAX
		e.preventDefault ? e.preventDefault() : e.returnValue = false;
		var callbackScript = $(this).attr("data-callback") || "";
		top.fsAjaxLoad($(this).attr('href'), $(this).attr('rel'), $(this).attr('params'), function(){
			try {
				if(callbackScript != "")
				{
					eval(callbackScript);
				}
			} catch (exception){
				// do nothing
			}
		});
	});

	$(document).on("click touchstart",".fs-order-btn",function(e){
		e.preventDefault ? e.preventDefault() : e.returnValue = false;
		
		var id = $(this).attr("data-id"); 
		
		if($("#modal-order-popup").length == 0)
		{
			var modal = $("<div id='modal-order-popup'></div>");
			modal.appendTo("body");
		}
		
		$.get("/ajax/order_form.php",{"id":id},function(html){
			$("#modal-order-popup").html(html);
			$("#modal-order-popup").popUpWindow({
				action: "open",
				modal: true,
				size: "small",
				buttons: [{
						text: "Отправить",
						cssClass: "fs-btn fs-btn-big",
						click: function(){
							$("#fs-order-form").submit();
						}
					}],
			});
		});
	});
	
	$(document).on("submit","#fs-order-form",function(e){
		e.preventDefault ? e.preventDefault() : e.returnValue = false;
		var formData = $(this).serializeArray();
		$.post($(this).attr("action"),formData,function(html){
			$("#modal-order-popup").html(html);
			if($("#modal-order-popup .fs-alert").length > 0)
			{
				$('html,body').animate({scrollTop: $("#modal-order-popup").eq(0).offset().top},400);
			}
		});
	});
	
	$(document).on("click",".fs-buy-btn",function(e){
		e.preventDefault ? e.preventDefault() : e.returnValue = false;
		var obj = $(this);
		var inbasketID = $(this).attr("data-inbasket");
		$.post("/ajax.php",{"action":"add2cart","id":$(this).attr("data-id")},function(response){
			if(response == "success") {
				if($("#"+inbasketID).length > 0) {
					obj.hide();
					$("#"+inbasketID).show();
				}
				if(typeof top.updateBasketBlock === "function") top.updateBasketBlock();
			}
		});
	});
	
	$(document).on("click touchstart",".fs-tabs-switcher .fs-btn",function(e){
		if($(this).prop("tagName") != "A")
		{
			e.preventDefault ? e.preventDefault() : e.returnValue = false;
			var switcher = $(this).closest(".fs-tabs-switcher");
			switcher.find(".fs-btn").addClass("fs-btn-white");
			$(this).removeClass("fs-btn-white");
			var ajaxHref = $(this).attr("data-ajax") || "";
			var tab = $(this).attr("data-tab") || "";
			var tabContainerID = switcher.attr("data-tabs") || "";
			if(tabContainerID.length > 0 && tab.length > 0)
			{
				$("#"+tabContainerID+">div").hide();
				var curTab = $("#"+tabContainerID+">div[rel="+tab+"]"); 
				curTab.show();
				if(ajaxHref.length > 0)
				{
					curTab.html('<center><span class="fs-loading-spinner"></span></center>');
					$.get(ajaxHref,{},function(html){
						curTab.html(html);
					});
				}
			}
		}
	});
	
	/*if($(".fs-tabs-switcher").length > 0)
	{
		$(".fs-tabs-switcher").each(function(){
			$(this).find(".fs-btn").not(".fs-btn-white").click();
		});
	}*/
	
	$(document).on("click touchstart",".fs-comment-rate",function(e){
		e.preventDefault ? e.preventDefault() : e.returnValue = false;
		var wr = $(this).closest(".fs-comment-rate-wr");
		$.post("/ajax/detail_comments.php",{"ajaxCommentsAction":"rate","id":$(this).attr("data-id"),"value":$(this).attr("data-val")},function(responseHTML){
			wr.html(responseHTML);
		});
		
	});
	
	
	$(document).on("click touchstart",".fs-add-comment",function(e){
		e.preventDefault ? e.preventDefault() : e.returnValue = false;
		var id = $(this).attr("data-id"); 
		if($("#modal-prod-comment-popup").length == 0)
		{
			var modal = $("<div id='modal-prod-comment-popup'></div>");
			modal.appendTo("body");
		}
        var needAuth = $(this).attr("data-needauth");
        if (needAuth == 1) {
            var buttons = [];
        } else {
            var buttons = [{
                text: "Отправить",
                cssClass: "fs-btn fs-btn-big",
                click: function () {
                    $("#fs-prod-comment-form").submit();
                }
            }];
        }

		$.get("/ajax/product_comment_add.php",{"id":id},function(html){
			$("#modal-prod-comment-popup").html(html);
			$("#modal-prod-comment-popup").popUpWindow({
				action: "open",
				modal: true,
				size: "small",
                buttons: buttons,
			});
		});
	});
	
	$(document).on("submit","#fs-prod-comment-form",function(e){
		e.preventDefault ? e.preventDefault() : e.returnValue = false;

		var formData = new FormData();
		Array.prototype.forEach.call(this.elements, function(el) {
			if (!el.name) return;
			formData.append(el.name, el.value);
		});

		window.reviewImages.forEach(function(image) {
			formData.append("PHOTOS[]", image, image.name);
		});

		var form = this;
		form.classList.add("preloader");

		$.ajax({
			url: $(this).attr("action"),
			data: formData,
			type: "POST",
			processData: false,
			contentType: false,
			error: function(err) {
				console.error(err);
				form.classList.remove("preloader");
			}
		}).done(function(html) {
			$("#modal-prod-comment-popup").html(html);
			if($("#modal-prod-comment-popup .fs-alert").length > 0)
			{
				$('html,body').animate({scrollTop: $("#modal-prod-comment-popup").eq(0).offset().top},400);
			}
		});
	});
	
	$(document).on("click touchstart",".fs-qa-add",function(e){
		e.preventDefault ? e.preventDefault() : e.returnValue = false;
		var id = $(this).attr("data-product-id"); 
		var qID = $(this).attr("data-parent-id"); 
		if($("#modal-prod-qa-popup").length == 0)
		{
			var modal = $("<div id='modal-prod-qa-popup'></div>");
			modal.appendTo("body");
		}
		$.get("/ajax/product_qa_add.php",{"id":id,"parentID":qID},function(html){
			$("#modal-prod-qa-popup").html(html);
			$("#modal-prod-qa-popup").popUpWindow({
				action: "open",
				modal: true,
				size: "small",
				buttons: [{
					text: "Отправить",
					cssClass: "fs-btn fs-btn-big",
					click: function(){
						$("#fs-prod-qa-form").submit();
					}
				}],
			});
		});
	});
	
	$(document).on("submit","#fs-prod-qa-form",function(e){
		e.preventDefault ? e.preventDefault() : e.returnValue = false;
		var formData = $(this).serializeArray();
		$.post($(this).attr("action"),formData,function(html){
			$("#modal-prod-qa-popup").html(html);
			if($("#modal-prod-qa-popup .fs-alert").length > 0)
			{
				$('html,body').animate({scrollTop: $("#modal-prod-qa-popup").eq(0).offset().top},400);
			}
		});
	});
	
	$(document).on("click touchstart",".fs-qa-rate",function(e){
		e.preventDefault ? e.preventDefault() : e.returnValue = false;
		var wr = $(this).closest(".fs-qa-rate-wr");
		$.post("/ajax/detail_qa.php",{"ajaxQaAction":"rate","id":$(this).attr("data-id"),"value":$(this).attr("data-val")},function(responseHTML){
			wr.html(responseHTML);
		});
	});
	
	
	$(document).on("click touchstart",".fs-qa-report",function(e){
		e.preventDefault ? e.preventDefault() : e.returnValue = false;
		var btn = $(this);
		swal({
			title: "Пожаловаться?",
			text: "Вы действительно хотите отправить жалобу?",
			type: "warning",
			showCancelButton: true,
			confirmButtonColor: "#DD6B55",
			confirmButtonText: "Да",
			cancelButtonText: "Нет",
			closeOnConfirm: false,
			closeOnCancel: true,
			}, function(isConfirm){
				if (isConfirm) {
					$.post("/ajax/detail_qa.php",{"ajaxQaAction":"sendComplainDiscus","id":btn.attr("data-id")},function(responseHTML){
						if(responseHTML == "OK")
						{
							swal("Жалоба отправлена!","","success");
						}
						else
						{
							swal("Ошибка",responseHTML,"error");
						}
					});
				}
			}
		);
	});
	
	
	$(document).on("click touchstart",".fs-list-switcher-item.active",function(e){
		e.preventDefault ? e.preventDefault() : e.returnValue = false;
		$(this).closest(".fs-list-switcher").toggleClass("active");
	});

	$(document).on("click touchstart",".fs-list-switcher-item:not(.fs-list-switcher-item.active)",function(e){
		e.preventDefault ? e.preventDefault() : e.returnValue = false;
		var wr = $(this).closest(".fs-list-switcher");
		wr.find(".fs-list-switcher-item").removeClass("active");
		wr.removeClass("active");
		$(this).addClass("active");
		$(this).prependTo(wr.find(".fs-list-switcher-body"));
		var target = $(this).attr("data-target") || "";
		var value = $(this).attr("data-value") || "";
		if(target.length > 0) {
			$(target).val(value);
			$(target).change();
		}
	});
	
	
	$(".fs-carousel-small").owlCarousel({
		loop:false,
		margin:10,
		nav:true,
		dots:false,
		responsive:{
			0:{items:1},
			600:{items:2},
			1000:{items:3},
			1300:{items:4}
		}
	});
	
	$(document).on("click touchstart",".fs-btn-toggle",function(e){
		e.preventDefault ? e.preventDefault() : e.returnValue = false;
		var rel = $(this).attr("rel") || "";
		if(rel.length > 0)
		{
			$(".fs-btn-toggle-content[rel="+rel+"]").toggle();
		}
		
	});

	$(document).on("click touchstart",".fs-price-alert-btn",function(e){
		e.preventDefault ? e.preventDefault() : e.returnValue = false;
		var wr = $(this).parent();
		var id = $(this).attr("data-id") || "";
		var action = $(this).attr("data-action") || "";
		if(id.length > 0)
		{
			$.post("/ajax/pricealert.php",{"id":id,"priceAlertAction":action},function(responseHTML){
				wr.html(responseHTML);
			});
		}
		
	});
	
	
	$(document).on("click touchstart","#fs-goto-fullversion-link",function(e){
		e.preventDefault ? e.preventDefault() : e.returnValue = false;
		var href = $(this).attr("href"); 
		swal({
			title: "Перейти на полную версию сайта?",
			showCancelButton: true,
			confirmButtonColor: "#DD6B55",
			confirmButtonText: "Да",
			cancelButtonText: "Нет",
			closeOnConfirm: false
		}, function(){
			//var cookiePrefix = phpVars.cookiePrefix || "";
			var myDate = new Date();
			myDate.setTime(myDate.getTime() + (10 * 86400 * 1000));
			document.cookie = "FOTOSKLAD_FULL_VERSION=true;expires=" + myDate.toGMTString() + ";domain=fotosklad.ru;path=/";
			document.location = href;
		});
	});
	
	
});
function initMaskInput(params)
{
	function setMaskPlaceholder($item, mask, placeholder)
	{
		$item.mask(mask);
		$item.attr('placeholder', placeholder);
	}
	function setCountryParams()
	{
		var $phones = arguments[0]
			mask = '+7(hhh)hhh-hh-hh',
			placeholder = '+7(___) ___-__-__';

		switch ($(this).val()) {
			case "Russia":
				mask = "+7(hhh)hhh-hh-hh";
				placeholder = "+7(___) ___-__-__";
				break;
			case "Belarus":
				mask = "+375(hh)hh-hh-hh";
				placeholder = "+375(__) ___-__-__";
				break;
			case "Kazakhstan":
				mask = "+7(hhh)hhh-hh-hh";
				placeholder = "+7(___) ___-__-__";
				break;
			case "Kirgizia":
				mask = "+996(hhh) hh-hh-hh";
				placeholder = "+996(___) ___-__-__";
				break;
			case "Armenia":
				mask = "+374(hh) hh-hh-hh";
				placeholder = "+374(__) ___-__-__";
				break;

		}
		$phones.each(function(indPhone, itemPhone){
			setMaskPlaceholder($(itemPhone), mask, placeholder);
		});
	}
	var defaultParams = {
			container: '.js-mask_container',
			country: '.js-mask_country',
			phone: '.js-mask_phone'
		},
		useParams = {};
	if (params)
	{
		for (var k in defaultParams)
		{
			useParams[k] = typeof params[k] == 'undefined' ? defaultParams[k] : params[k];
		}
	}
	else
	{
		useParams = defaultParams;
	}

	var $container = $(useParams.container);

	$container.each(function(indContainer, itemContainer){
		var $phones = $(itemContainer).find(useParams.phone),
			$country = $(itemContainer).find(useParams.country);
		$country.each(function(indCountry, itemCountry){
			$(itemCountry).change($.proxy(setCountryParams, $(itemCountry), $phones));
		});
		$(itemContainer).find(useParams.country+':checked').trigger('change');
	});
}

/**
 * Add2cart popup
 */
$(document).ready(function () {
    let currentRequest = null;
    $(document).on('click', '.add2cart_popup', function (e) {
        e.preventDefault ? e.preventDefault() : e.returnValue = false;
        let $btn = $(e.target);
        currentRequest = $.ajax({
            type: 'GET',
            url: '/ajax/add2cart_popup.php',
            data: {'product_id': $(this).data('id')},
            beforeSend: function() {
                if (currentRequest != null) {
                    currentRequest.abort();
                }
                $btn.addClass('btn--action-started');
            },
            success: function (data) {
                let $container = $('#add2cart_popup').length ? $('#add2cart_popup') : $('<div id="add2cart_popup"></div>').appendTo('body');
                $container.html(data);
            }
        }).always(function () {
            $btn.removeClass('btn--action-started');
        });
    });
});
